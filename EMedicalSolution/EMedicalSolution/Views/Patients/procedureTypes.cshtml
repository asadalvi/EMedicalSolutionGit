@model EMedicalSolution.ViewModels.patientIntakeViewModel
<h2>Step 2 Content</h2>
@using (Html.BeginForm("procedureTypes", "Patients", FormMethod.Post, new { id = "popupForm" }))
{
    if (Model != null && Model.objProcedureType != null)
    {
        foreach (var procedure in Model.objProcedureType)
        {

        <input class="w3-radio procedure_Type" type="checkbox" name="procedureType" value=@procedure.ID checked />
                   @procedure.Title
            <br />
        }
        <input type="hidden" name="pHistoryId" value="69" />
    }
    <div class="w3-row  w3-margin-top">
            <input class="w3-btn-block w3-green" type="submit" value="Save" />
        </div>
}
